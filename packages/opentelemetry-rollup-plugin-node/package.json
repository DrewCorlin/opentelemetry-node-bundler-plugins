{
  "name": "opentelemetry-rollup-plugin-node",
  "description": "Rollup plugin which bundles opentelemetry node core and contrib instrumentations into your code",
  "version": "4.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "nx": {
    "targets": {
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "packages/opentelemetry-rollup-plugin-node",
          "command": "rm -rf dist"
        }
      }
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "opentelemetry-node-bundler-plugin-utils": "0.5.0",
    "rollup": "^4.41.1"
  }
}
